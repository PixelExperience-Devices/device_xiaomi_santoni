typeattribute vendor_init data_between_core_and_vendor_violators;

set_prop(vendor_init, fm_prop)

allow vendor_init rootfs:lnk_file setattr;
allow vendor_init fpc_data_file:dir create_file_perms;

allow vendor_init {
  bluetooth_data_file
  camera_data_file
  dhcp_data_file
  media_rw_data_file
  system_data_file
  time_data_file
  tombstone_data_file
  wifi_data_file
  wpa_socket
  rootfs
}:dir { create search getattr open read setattr ioctl write add_name remove_name rmdir relabelfrom };

allow vendor_init proc_dirty:file { write };
allow vendor_init proc_dirty_ratio:file { write };
allow vendor_init proc_random:file { write };
